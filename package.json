{
	"name": "insert-new-line",
	"displayName": "Insert New Line",
	"description": "Insert new line with any key stroke.",
	"version": "0.1.1",
	"publisher": "yeannylam",
	"engines": {
		"vscode": "^1.34.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/zhangkaiyulw/insert-new-line.git"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:insertNewLine",
		"onCommand:openNewLine"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "insertNewLine",
				"title": "Insert New Line"
			},
			{
				"command": "openNewLine",
				"title": "Open New Line"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
